// MY FIRST C PROGRAM

#include <stdio.h>
int main()
{
  
 int a=2;
    
double const b =4;

    float c = 1.5;
  
  char d = 'A';
 
 printf("The value of a is %d\n",a);
 
 printf("The value of a is %lf\n",b);
   
 printf("The value of a is %d.2f\n",c);
  
 printf("The value of a is %c\n",d);

 
  return 0;

}




The value of a is 2
the value of a is 4
the value of a is 1549.2f
the value of a is A
